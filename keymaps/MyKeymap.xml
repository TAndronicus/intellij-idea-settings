<keymap version="1" name="MyKeymap" parent="$default">
  <action id="ActivateDBExecutionConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt 8" />
  </action>
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateFavoritesToolWindow" />
  <action id="ActivateGradleToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateMavenProjectsToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateMavenToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateMessagesToolWindow" />
  <action id="ActivateTODOToolWindow" />
  <action id="ActivateTerminalToolWindow" />
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="SelectInProjectView">
    <keyboard-shortcut first-keystroke="shift subtract" />
  </action>
  <action id="SelectNextOccurrence" />
</keymap>